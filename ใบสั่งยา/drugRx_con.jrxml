	<parameter name="doctor_id" isForPrompting="false" class="java.lang.String"/>
	<parameter name="list_order" isForPrompting="false" class="java.lang.String"/>
	<parameter name="t_visit_id" isForPrompting="false" class="java.lang.String"/>
	<queryString><![CDATA[select
            t_visit.visit_hn as hn
            ,t_visit.visit_vn as vn
            ,f_patient_prefix.patient_prefix_description||' '||t_patient.patient_firstname ||'  '|| t_patient.patient_lastname as name
            ,f_sex.sex_description as sex
            ,substring(t_visit.visit_begin_visit_time,1,10)   as visit_date
            ,substring(t_visit.visit_begin_visit_time,12,5) as visit_time
            ,t_patient.patient_pid as pid
            ,t_patient.patient_xn as xn
            ,f_visit_type.visit_type_description as patient_type
            ,b_contract_plans.contract_plans_description as plan
            ,t_visit_payment.visit_payment_card_number AS "planid"
            ,t_patient.patient_house as house
            ,t_patient.patient_moo as moo
            ,f1.address_description as tambon
            ,f2.address_description as auphur
            ,f3.address_description  as changwat
            ,main_hospital.visit_office_name2 || main_hospital.visit_office_name1 as mainhospital
            ,sub_hospital.visit_office_name2 || sub_hospital.visit_office_name1 as subhospital
            ,replace(t_visit.visit_dx,'\n',' , ') as dx
            ,replace(t_visit.visit_diagnosis_notice,'\n',' , ') as dx_note
            ,t_visit.visit_patient_age as age
            ,icd10.diag_icd10 as diag_icd10
            ,icd10.disease_th as disease_th
            , case when visit_vital_sign.vital_sign_weight  is null then 'ไม่ระบุ' else visit_vital_sign.vital_sign_weight end as vital_sign_weight
            , case when visit_vital_sign.vital_sign_height is null then 'ไม่ระบุ' else visit_vital_sign.vital_sign_height end as vital_sign_height

            ,case when t_patient.patient_drugallergy='1' then 'มีประวัติการแพ้ยา' else '' end as allergy
            ,patient_drug_allergy.drug_allergy as drug_allergy

            ,b_item_billing_subgroup.item_billing_subgroup_description as billing_subgroup
            ,t_order.order_common_name as common_name
            ,uom_use.item_drug_uom_description as dispense_unit
            ,case when b_item_drug_instruction.item_drug_instruction_description is null or b_item_drug_instruction.item_drug_instruction_description = ''
                        then ''
                        else b_item_drug_instruction.item_drug_instruction_description

                    ||' '||(case when b_item_drug_dose.item_drug_dose_description is null and t_order_drug.order_drug_dose::numeric <> 0
                                            then (t_order_drug.order_drug_dose::numeric)||' '||uom_use.item_drug_uom_description
                                    when b_item_drug_dose.item_drug_dose_description is not null and t_order_drug.order_drug_dose::numeric::int > 1
                                            then  floor(t_order_drug.order_drug_dose::numeric)||' '||uom_use.item_drug_uom_description
                                                            ||b_item_drug_dose.item_drug_dose_description
                                    when b_item_drug_dose.item_drug_dose_description is not null
                                            then b_item_drug_dose.item_drug_dose_description||uom_use.item_drug_uom_description
                                            else ''
                                    end)
                    end ||'  '|| b_item_drug_frequency.item_drug_frequency_description as long_dose
--,"b_item_drug_instruction"."item_drug_instruction_number" || ' ' || ("t_order_drug".order_drug_dose::numeric) || ' ' ||"b_item_drug_frequency"."item_drug_frequency_number" AS "short_dose"

,case when t_order_drug.order_drug_special_prescription_text <> '' then REGEXP_REPLACE(t_order_drug.order_drug_special_prescription_text, '[\r\n]+', ' ', 'g') else t_order_drug.order_drug_dose||' '||uom_use.item_drug_uom_description||' x '||b_item_drug_frequency.item_drug_frequency_number end as "short_dose"


            ,sum(t_order.order_qty) as qty
            ,uom_purch.item_drug_uom_description as uom_purch
            ,sum(t_order.order_qty  * t_order.order_price) as price

            ,case when person_verify.t_person_id is not null and prefix_verify.f_patient_prefix_id <> '000'
                        then prefix_verify.patient_prefix_description||person_verify.person_firstname||'  '||person_verify.person_lastname
                    when person_verify.t_person_id is not null and prefix_verify.f_patient_prefix_id = '000'
                        then person_verify.person_firstname||'  '||person_verify.person_lastname
                        else ''
                        end as staff_verify

            ,case when person_execute.t_person_id is not null and prefix_execute.f_patient_prefix_id <> '000'
                        then prefix_execute.patient_prefix_description||person_execute.person_firstname||'  '||person_execute.person_lastname
                    when person_execute.t_person_id is not null and prefix_execute.f_patient_prefix_id = '000'
                        then person_execute.person_firstname||'  '||person_execute.person_lastname
                        else ''
                        end as staff_execute

            ,(to_number(to_char(current_date, 'YYYY'), '9999') + 543) || to_char(current_timestamp, '-MM-DD') as print_date
            ,to_char(current_timestamp, 'HH24:MI:SS') as print_time

            ,case when person_staff_lock.t_person_id is not null and prefix_staff_lock.f_patient_prefix_id <> '000'
                        then prefix_staff_lock.patient_prefix_description||person_staff_lock.person_firstname||'  '||person_staff_lock.person_lastname
                    when person_staff_lock.t_person_id is not null and prefix_staff_lock.f_patient_prefix_id = '000'
                        then person_staff_lock.person_firstname||'  '||person_staff_lock.person_lastname
                        else ''
                        end as staff_lock

              ,(case when  $P{doctor_id} <> ''
                            then (select
                                                case when t_person.t_person_id is not null and f_patient_prefix.f_patient_prefix_id <> '000'
                                                            then f_patient_prefix.patient_prefix_description||t_person.person_firstname||'  '||t_person.person_lastname
                                                        when t_person.t_person_id is not null and f_patient_prefix.f_patient_prefix_id = '000'
                                                            then t_person.person_firstname||'  '||t_person.person_lastname
                                                            else ''
                                                            end as firstname_lastname
                                            from
                                                     b_employee left join t_person on b_employee.t_person_id = t_person.t_person_id
                                                     left join f_patient_prefix on t_person.f_prefix_id = f_patient_prefix.f_patient_prefix_id
                                            where b_employee.b_employee_id = $P{doctor_id})
                               else ''
                               end) as staff_doctor

            ,perdoc.person_firstname||'  '||perdoc.person_lastname as taff_doctor
            ,b1.employee_number as doc_number
            ,b_site.site_full_name as hospital

						,ckd_lab.ckd_value as ckd
						,inr_lab.inr_value as inr
						,sgpt_lab.sgpt_value as sgpt
						,sgot_lab.sgot_value as sgot


from
        t_visit left join t_order on t_visit.t_visit_id = t_order.t_visit_id
                                            and t_order.t_order_id in ($P!{list_order})
                                            and t_order.f_order_status_id not in ('0','3')

        left join b_employee as staff_verify on t_order.order_staff_verify = staff_verify.b_employee_id
        left join t_person as person_verify on staff_verify.t_person_id = person_verify.t_person_id
        left join f_patient_prefix as prefix_verify on person_verify.f_prefix_id = prefix_verify.f_patient_prefix_id

        left join b_employee as staff_execute on t_order.order_staff_execute = staff_execute.b_employee_id
        left join t_person as person_execute on staff_execute.t_person_id = person_execute.t_person_id
        left join f_patient_prefix as prefix_execute on person_execute.f_prefix_id = prefix_execute.f_patient_prefix_id

        left join b_item_billing_subgroup on t_order.b_item_billing_subgroup_id = b_item_billing_subgroup.b_item_billing_subgroup_id
        left join t_order_drug on t_order.t_order_id = t_order_drug.t_order_id and t_order_drug.order_drug_active = '1'
        left join b_item_drug_instruction on t_order_drug.b_item_drug_instruction_id = b_item_drug_instruction.b_item_drug_instruction_id
        left join b_item_drug_frequency on t_order_drug.b_item_drug_frequency_id = b_item_drug_frequency.b_item_drug_frequency_id
        left join b_item_drug_uom as uom_use on t_order_drug.b_item_drug_uom_id_use = uom_use.b_item_drug_uom_id
        left join b_item_drug_uom as uom_purch on t_order_drug.b_item_drug_uom_id_purch = uom_purch.b_item_drug_uom_id

        left join f_visit_type on t_visit.f_visit_type_id = f_visit_type.f_visit_type_id

        left join (select
                                t_diag_icd10.diag_icd10_vn as diag_icd10_vn
                                , array_to_string(array_agg(diag_icd10_number),', ') as diag_icd10
                                , array_to_string(array_agg(b_icd10.icd10_description),'/ ') as disease_th
                        from
                                t_diag_icd10 left join b_icd10 on t_diag_icd10.diag_icd10_number = b_icd10.icd10_number
                        where
                                t_diag_icd10.diag_icd10_active = '1'
                                and t_diag_icd10.f_diag_icd10_type_id ='1'
                                and t_diag_icd10.diag_icd10_vn = $P{t_visit_id}

                        group by
                                t_diag_icd10.diag_icd10_vn) as icd10
                        on t_visit.t_visit_id = icd10.diag_icd10_vn

          left join (select
                        b_item_drug_uom.b_item_drug_uom_id as b_item_drug_uom_id
                        ,b_item_drug_dose.item_drug_dose_value as item_drug_dose_value
                        ,b_item_drug_dose.item_drug_dose_description as item_drug_dose_description
                from b_item_drug_uom
                        left join b_item_drug_dose_map_uom on b_item_drug_uom.b_item_drug_uom_id = b_item_drug_dose_map_uom.b_item_drug_uom_id
                        left join b_item_drug_dose on b_item_drug_dose_map_uom.b_item_drug_dose_id = b_item_drug_dose.b_item_drug_dose_id) as b_item_drug_dose
                                     on uom_use.b_item_drug_uom_id = b_item_drug_dose.b_item_drug_uom_id
                                    and (case when t_order_drug.order_drug_dose::numeric::int > 1
                                                    then  t_order_drug.order_drug_dose::numeric - floor(t_order_drug.order_drug_dose::numeric)
                                                    else t_order_drug.order_drug_dose::numeric
                                                    end) = b_item_drug_dose.item_drug_dose_value::numeric


        left join t_visit_payment on t_visit_payment.t_visit_id = t_visit.t_visit_id and t_visit_payment.visit_payment_priority = '0' and t_visit_payment.visit_payment_active = '1'
        left join b_contract_plans b_contract_plans on t_visit_payment.b_contract_plans_id = b_contract_plans.b_contract_plans_id
        left join b_visit_office as main_hospital on t_visit_payment.visit_payment_main_hospital = main_hospital.b_visit_office_id
        left join b_visit_office as sub_hospital on t_visit_payment.visit_payment_sub_hospital = sub_hospital.b_visit_office_id
        left join t_patient on t_patient.t_patient_id = t_visit.t_patient_id
        left join f_patient_prefix on f_patient_prefix.f_patient_prefix_id = t_patient.f_patient_prefix_id
        left join f_sex on f_sex.f_sex_id = t_patient.f_sex_id
        LEFT join f_address as f1 on t_patient.patient_tambon = f1.f_address_id
        LEFT join f_address as f2 on t_patient.patient_amphur = f2.f_address_id
        LEFT join f_address as f3 on t_patient.patient_changwat = f3.f_address_id

        left join
                    (select
                                t_patient_drug_allergy.t_patient_id as t_patient_id
                                ,array_to_string(array_agg(t_patient_drug_allergy.allergy_warning||'\t'||t_patient_drug_allergy.drug_allergy ),'\n') as drug_allergy
                        from
                                                (select
                                                        t_patient_drug_allergy.t_patient_id as t_patient_id
                                                        ,f_allergy_warning_type.f_allergy_warning_type_id as f_allergy_warning_type_id
                                                        ,f_allergy_warning_type.warning_type_description  as allergy_warning
                                                        ,array_to_string(array_agg(b_item_drug_standard.item_drug_standard_description ),', ') as drug_allergy
                                                from t_patient_drug_allergy left join b_item_drug_standard
                                                        on t_patient_drug_allergy.b_item_drug_standard_id = b_item_drug_standard.b_item_drug_standard_id
                                                        and t_patient_drug_allergy.active = '1'
                                                        left join f_allergy_warning_type on t_patient_drug_allergy.f_allergy_warning_type_id = f_allergy_warning_type.f_allergy_warning_type_id
                                                group by
                                                         t_patient_drug_allergy.t_patient_id
                                                         ,f_allergy_warning_type.f_allergy_warning_type_id
                                                         ,f_allergy_warning_type.warning_type_description
                                                order by
                                                        f_allergy_warning_type_id desc) as t_patient_drug_allergy
                        group by
                                t_patient_drug_allergy.t_patient_id ) as patient_drug_allergy on t_patient.t_patient_id = patient_drug_allergy.t_patient_id


        left join (select
                               t_visit_vital_sign.t_visit_id as t_visit_id
                               ,case when t_visit_vital_sign.visit_vital_sign_weight= '' then 'ไม่ระบุ' else t_visit_vital_sign.visit_vital_sign_weight--||' กก.'
end  as vital_sign_weight
                                ,case when t_visit_vital_sign.visit_vital_sign_height= '' then 'ไม่ระบุ' else t_visit_vital_sign.visit_vital_sign_height||' ซม.' end  as vital_sign_height
                        from
                                t_visit_vital_sign inner join (select
                                t_visit_vital_sign.t_visit_id as visit_id
                                ,max(t_visit_vital_sign.record_date||','||t_visit_vital_sign.record_time)  as   record_date
                        from
                                t_visit_vital_sign
                        where
                                t_visit_vital_sign.visit_vital_sign_active = '1'

                        group by
                                t_visit_vital_sign.t_visit_id) as vital_sign on vital_sign.visit_id = t_visit_vital_sign.t_visit_id and vital_sign.record_date = (t_visit_vital_sign.record_date||','||t_visit_vital_sign.record_time)
                        where
                                t_visit_vital_sign.visit_vital_sign_active ='1' ) as visit_vital_sign
            on visit_vital_sign.t_visit_id = t_visit.t_visit_id

        left join b_employee as visit_staff_lock on t_visit.visit_staff_lock = visit_staff_lock.b_employee_id
        left join t_person as person_staff_lock on visit_staff_lock.t_person_id = person_staff_lock.t_person_id
        left join f_patient_prefix as prefix_staff_lock on person_staff_lock.f_prefix_id = prefix_staff_lock.f_patient_prefix_id
        LEFT JOIN "public"."t_diag_icd10" "t_diag_icd10" ON ("t_diag_icd10"."diag_icd10_vn" = "t_visit"."t_visit_id"  and "t_diag_icd10"."f_diag_icd10_type_id" ='1' and "t_diag_icd10"."diag_icd10_active" ='1')
        LEFT JOIN b_employee as b1 ON t_diag_icd10.diag_icd10_staff_doctor = b1.b_employee_id
        left join t_person as perdoc on (perdoc.t_person_id = b1.t_person_id and perdoc.active = '1')


				LEFT JOIN (
				SELECT
ckd_order.t_patient_id as id
,ckd.result_lab_name as Lab_ckd
,ckd.record_date_time
,(ckd.result_lab_value) as ckd_value

from t_patient
LEFT JOIN t_order as ckd_order on t_patient.t_patient_id = ckd_order.t_patient_id
LEFT JOIN t_result_lab as ckd on (ckd_order.t_order_id = ckd.t_order_id and ckd.result_lab_name like '%CKD%')

WHERE ckd_order.t_patient_id = (SELECT t_patient_id FROM t_visit WHERE t_visit_id = $P{t_visit_id} )

AND result_lab_active = '1'

ORDER BY ckd.record_date_time DESC
fetch first rows only
				) as ckd_lab on ckd_lab.id = t_patient.t_patient_id


				LEFT JOIN (
				SELECT
inr_order.t_patient_id as id
,inr.result_lab_name as Lab_inr
,inr.record_date_time
,(inr.result_lab_value) as inr_value

from t_patient
LEFT JOIN t_order as inr_order on t_patient.t_patient_id = inr_order.t_patient_id
LEFT JOIN t_result_lab as inr on (inr_order.t_order_id = inr.t_order_id and inr.result_lab_name like '%INR%')

WHERE inr_order.t_patient_id = (SELECT t_patient_id FROM t_visit WHERE t_visit_id = $P{t_visit_id} )

AND result_lab_active = '1'

ORDER BY inr.record_date_time DESC
fetch first rows only
				) as inr_lab on inr_lab.id = t_patient.t_patient_id


									LEFT JOIN (
				SELECT
sgpt_order.t_patient_id as id
,sgpt.result_lab_name as Lab_sgpt
,sgpt.record_date_time
,(sgpt.result_lab_value) as sgpt_value

from t_patient
LEFT JOIN t_order as sgpt_order on t_patient.t_patient_id = sgpt_order.t_patient_id
LEFT JOIN t_result_lab as sgpt on (sgpt_order.t_order_id = sgpt.t_order_id and sgpt.result_lab_name like '%SGPT%')

WHERE sgpt_order.t_patient_id = (SELECT t_patient_id FROM t_visit WHERE t_visit_id = $P{t_visit_id} )

AND result_lab_active = '1'

ORDER BY sgpt.record_date_time DESC
fetch first rows only
				) as sgpt_lab on sgpt_lab.id = t_patient.t_patient_id

									LEFT JOIN (
				SELECT
sgot_order.t_patient_id as id
,sgot.result_lab_name as Lab_sgpt
,sgot.record_date_time
,(sgot.result_lab_value) as sgot_value

from t_patient
LEFT JOIN t_order as sgot_order on t_patient.t_patient_id = sgot_order.t_patient_id
LEFT JOIN t_result_lab as sgot on (sgot_order.t_order_id = sgot.t_order_id and sgot.result_lab_name like '%SGOT%')

WHERE sgot_order.t_patient_id = (SELECT t_patient_id FROM t_visit WHERE t_visit_id = $P{t_visit_id} )

AND result_lab_active = '1'

ORDER BY sgot.record_date_time DESC
fetch first rows only
				) as sgot_lab on sgot_lab.id = t_patient.t_patient_id


        cross join b_site

where
       t_visit.t_visit_id = $P{t_visit_id}

group by
        hn
        ,vn
        ,name
        ,sex
        ,visit_date
        ,visit_time
        ,pid
        ,xn
        ,patient_type
        ,plan
        ,mainhospital
        ,subhospital
        ,dx
        ,dx_note
        ,age
        ,diag_icd10
        ,disease_th
        ,vital_sign_weight
        ,vital_sign_height
        ,allergy
        ,drug_allergy
        ,billing_subgroup
        ,common_name
        ,dispense_unit
        ,long_dose
        ,uom_purch
        --,qty
        --,price
        ,staff_verify
        ,staff_execute
        ,print_date
        ,print_time
        ,staff_lock

				,staff_doctor

				,hospital
,t_visit_payment.visit_payment_card_number
,t_patient.patient_house
,t_patient.patient_moo
,f1.address_description
,f2.address_description
,f3.address_description
,b_item_drug_instruction.item_drug_instruction_number
,t_order_drug.order_drug_dose
,b_item_drug_frequency.item_drug_frequency_number
,b1.employee_number
,perdoc.person_firstname
,perdoc.person_lastname

,ckd_lab.ckd_value
,inr_lab.inr_value
,sgpt_lab.sgpt_value
,sgot_lab.sgot_value
,t_order.order_date_time

,t_order_drug.order_drug_special_prescription_text
,b_item_drug_instruction.item_drug_instruction_description
,uom_use.item_drug_uom_description
,b_item_drug_frequency.item_drug_frequency_description

order by
        billing_subgroup asc
        ,t_order.order_date_time asc]]></queryString>

	<field name="hn" class="java.lang.String"/>
	<field name="name" class="java.lang.String"/>
	<field name="sex" class="java.lang.String"/>
	<field name="visit_date" class="java.lang.String"/>
	<field name="visit_time" class="java.lang.String"/>
